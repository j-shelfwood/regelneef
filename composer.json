{
    "name": "shelfwood/regelneef",
    "description": "My own AutoGPT client",
    "keywords": [
        "framework",
        "laravel",
        "laravel zero",
        "console",
        "cli",
        "AI",
        "Automation",
        "GPT"
    ],
    "homepage": "https://shelfwood.co",
    "type": "project",
    "authors": [
        {
            "name": "Joris Schelfhout",
            "email": "joris@shelfwood.co"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0",
        "intonate/tinker-zero": "^1.2",
        "laminas/laminas-text": "^2.5",
        "laravel-zero/framework": "^10.0",
        "nunomaduro/termwind": "^1.15"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.22.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "regelneef"
    ]
}
